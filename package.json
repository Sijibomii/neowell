    "express": "^4.18.2",
    "glslify-loader": "^2.0.0",
    "gsap": "^3.11.4",
    "loadash": "^1.0.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "normalize-wheel": "^1.0.1",
    "ogl": "^0.0.109",
    "pug": "^3.0.2",
    "ua-parser-js": "^0.7.33"
  },
  "devDependencies": {
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
